# A2A Telegram Bot Configuration
# Copy this file to .env and fill in your values

# Required: Telegram Bot Token (get from @BotFather)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Required: Private key (with 0x prefix)
CLIENT_PRIVATE_KEY=0xyour_private_key_here

# Required: Worker Smart Account Address (the account being monitored)
WORKER_ADDRESS=0x164e7A98fa7Bd34679522c470bF68D66C5b00C66

# Optional: Polling interval in milliseconds (default: 5000)
POLL_INTERVAL=5000

# Optional: Payment amount for /hire command (default: 0.0001)
HIRE_AMOUNT=0.0001
